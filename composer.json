{
  "name": "adamhopkinson/laravel-model-hash",
  "description": "A trait which automatically generates a unique hash per model instance",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "trait",
    "model",
    "key",
    "id"
  ],
  "authors": [
    {
      "name": "Adam Hopkinson",
      "email": "adam.hopkinson@gmail.com"
    }
  ],
  "require": {
    "doctrine/dbal": "^2.10"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^9.2"
  },
  "autoload": {
    "psr-4": {
      "AdamHopkinson\\LaravelModelHash\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ],
    "psr-4": {
      "AdamHopkinson\\LaravelModelHash\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdamHopkinson\\LaravelModelHash\\LaravelModelHashServiceProvider"
      ]
    }
  }
}
